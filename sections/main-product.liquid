<section
  id="MainProduct-{{ section.id }}"
  class="section-{{ section.id }}-padding gradient color-{{ section.settings.color_scheme }}"
  data-section="{{ section.id }}"
>
  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>

  {{ 'next_product.css' | asset_url | stylesheet_tag }}

  {%- if product.quantity_price_breaks_configured? -%}
    <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  <div class="none_custom_image_info">
    {% comment %}
      {% assign array_of_images = product.metafields.custom.immagini_background.value %}
      {% if array_of_images %}
        {% for image in array_of_images %}
          <p class="background_url">https:{{ image | image_url }}</p>
        {% endfor %}
      {% endif %}
      {% assign array_of_images = product.metafields.custom.immagini_background.value %}
      {% if array_of_images %}
        {% for image in array_of_images %}
          <p class="background_url">https:{{ image | image_url }}</p>
        {% endfor %}
      {% endif %}
    {% endcomment %}

    {%- for media in product.media -%}
      {% assign image_type = media.src | split: '_' | slice: 3 | join: '' %}
      {% if image_type == 'TLM' %}
        {% assign color = media.src | split: '_' | slice: 1 | join: '' %}
        <p class="background_url" data-color="{{ color }}">{{ media | image_url: width: 3200 }}</p>
      {%- endif -%}
    {%- endfor -%}

    {% for variant in product.variants %}
      {% if variant == product.selected_or_first_available_variant %}
        <p class="codice_colore_attuale">{{ variant.metafields.custom.codice_colore }}</p>
      {% endif %}
    {% endfor %}

    {% assign unique_color_codes = '' %}
    {% for variant in product.variants %}
      {% assign codice_colore = variant.metafields.custom.codice_colore | upcase %}
      {% unless unique_color_codes contains codice_colore %}
        <p class="codice_colore">{{ codice_colore }}</p>
        {% assign unique_color_codes = unique_color_codes | append: codice_colore | append: ' ' %}
      {% endunless %}
    {% endfor %}
  </div>

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
  <div class="cont_principale" style="width: calc(100% - 1px);">
    <div class="product">
      <div
        class="product__media-wrapper"
      >
        {% render 'product-media-gallery', variant_images: variant_images %}
      </div>
      <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
        <product-info
          id="ProductInfo-{{ section.id }}"
          data-section="{{ section.id }}"
          data-url="{{ product.url }}"
          class="product__info-container{% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
        >
          {%- assign product_form_id = 'product-form-' | append: section.id -%}
    

          <div class="meta_box">
            {% render 'breadcrumbs_custom' %}
            <div>
              {% if product.metafields.custom.new_bedge == true %}
                <div class="product_bedge new_bedge">New</div>
              {% endif %}

              {% for collection in product.collections %}
                {% if collection.handle == 'private-sales' %}
                <div class="product_bedge private_bedge">PRIVATE SALES</div>
                {% endif %}
              {% endfor %}

              {%- for block in section.blocks -%}
                {%- case block.type -%}
                  {%- when 'price' -%}
                    <div class="no-js-hidden" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                      {%- render 'price',
                        product: product,
                        use_variant: true,
                        show_badges: true,
                        price_class: 'none_custom'
                      -%}
                    </div>
                {%- endcase -%}
              {% endfor %}
              {% render 'label_pro' %}
              {% render 'label_riciclato' %}
            </div>
          </div>

          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when '@app' -%}
                {% render block %}
              {%- when 'text' -%}
                <p
                  class="product__text inline-richtext{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}"
                  {{ block.shopify_attributes }}
                >
                  {{- block.settings.text -}}
                </p>
              {%- when 'title' -%}
                <div class="product__title" {{ block.shopify_attributes }}>
                  <h1 class="h2 italic">
                    {{ product.title | escape }}
                    <span>{{ product.metafields.custom.sottotitolo }}</span>
                  </h1>
                </div>
              {%- when 'description' -%}
                {%- if product.description != blank -%}
                  <div class="product__description rte quick-add-hidden" {{ block.shopify_attributes }}>
                    {{ product.description }}
                  </div>
                  {% render 'price_mobile_version' %}
                {%- endif -%}

                {% liquid
