                      assign privat_disclaimer = "If youâ€™re part of the SLAM Community, you can enjoy 40% off this product. Just enter the promo code you received via our Newsletter at checkout! Not a member yet? ðŸ‘‰"
                      assign privat_disclaimer_pt2 = "here to get your code and never miss future deals!"
                      assign privat_link = "https://www.slam.com/pages/landing-newsletter"
                      assign testo_link_privat = "Sign up"

                  endcase
                %}
                {%- assign variant = product.selected_or_first_available_variant -%}
                {%- if variant.metafields.custom.variant_exception_alert == true -%}
                  <p class="variant_alert">âš  {{ disclaimer }}</p>
                {%- endif -%}

                {% for collection in product.collections %}
                  {% if collection.handle == 'private-sales' %}
